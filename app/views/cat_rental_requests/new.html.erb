<form action="/cat_rental_requests" method="POST">
  Cat:<select name="cat_rental_request[cat_id]">
    <%Cat.all.each do |cat|%>
    <%= "<option value=#{cat.id}>#{cat.name}</option>".html_safe %>
    <%end%>
  </select><br>
  Rental start date:<input type="date" name="cat_rental_request[start_date]"><br />
  Rental end date:<input type="date" name="cat_rental_request[end_date]"><br />


  <input type="submit" value="Rent a cat">

</form>
